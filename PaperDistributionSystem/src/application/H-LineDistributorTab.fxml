<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HLineDistributorTabController">
   <children>
      <HBox prefHeight="571.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox prefHeight="571.0" prefWidth="1060.0" HBox.hgrow="ALWAYS">
               <children>
                  <VBox>
                     <children>
                        <HBox alignment="CENTER_RIGHT" spacing="15.0">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                           <children>
                              <TextField fx:id="addNameField" promptText="Name" />
                              <TextField fx:id="addMobileNumField" promptText="Mobile Number" />
                              <ComboBox fx:id="addLineNumField" prefWidth="150.0" promptText="Line Number" />
                              <Button mnemonicParsing="false" onAction="#addButtonClicked" text="Add Distributor" />
                              <Button mnemonicParsing="false" onAction="#resetClicked" text="Reset" />
                           </children>
                        </HBox>
                        <TableView fx:id="lineDistInfoTable" prefHeight="600.0" prefWidth="1280.0">
                           <columns>
                              <TableColumn fx:id="lineDistNameColumn" prefWidth="198.0" text="Name" />
                              <TableColumn fx:id="lineDistMobNumColumn" prefWidth="125.0" text="Mobile Number" />
                              <TableColumn fx:id="lineDistLineNumColumn" prefWidth="87.0" text="Line Number" />
                           </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
