<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AHawkerInfoTabController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="75.0" prefWidth="1280.0" spacing="50.0">
               <children>
                  <VBox spacing="10.0">
                     <children>
                        <HBox alignment="CENTER" spacing="25.0">
                           <children>
                              <TextField fx:id="addHwkName" promptText="Hawker Name" />
                              <TextField fx:id="addHwkCode" promptText="Hawker Code" />
                              <TextField fx:id="addHwkMob" promptText="Mobile Number" />
                              <TextField fx:id="addHwkAgencyName" layoutX="446.0" layoutY="10.0" promptText="Agency Name" />
                              <TextField fx:id="addHwkFee" prefHeight="25.0" prefWidth="103.0" promptText="Fee" />
                              <CheckBox fx:id="addHwkActive" mnemonicParsing="false" text="Active" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <TextField fx:id="addHwkOldHNum" prefHeight="25.0" prefWidth="73.0" promptText="Old House#" />
                              <TextField fx:id="addHwkNewHNum" layoutX="30.0" layoutY="12.0" prefHeight="25.0" prefWidth="73.0" promptText="New House#" />
                              <TextField fx:id="addHwkAddrLine1" promptText="Address Line 1" />
                              <TextField fx:id="addHwkAddrLine2" layoutX="363.0" layoutY="12.0" promptText="Address Line 2" />
                              <TextField fx:id="addHwkLocality" layoutX="537.0" layoutY="12.0" prefHeight="25.0" prefWidth="114.0" promptText="Locality" />
                              <TextField fx:id="addHwkCity" layoutX="537.0" layoutY="12.0" prefHeight="25.0" prefWidth="111.0" promptText="City" />
                              <ComboBox fx:id="addHwkState" prefWidth="150.0" promptText="State" visibleRowCount="30" />
                           </children>
                        </HBox>
                     </children>
                     <HBox.margin>
                        <Insets bottom="5.0" top="10.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox alignment="CENTER" spacing="15.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#addHawkerClicked" text="Add Hawker" />
                        <Button mnemonicParsing="false" onAction="#resetClicked" text="Reset" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" layoutX="1074.0" layoutY="10.0" spacing="15.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#filterHawkersClicked" text="Filter Hawkers" />
                        <Button mnemonicParsing="false" onAction="#clearClicked" text="Clear" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <TableView fx:id="hawkerTable" prefHeight="600.0" prefWidth="1280.0">
               <columns>
                  <TableColumn fx:id="hwkNameColumn" prefWidth="250.0" text="Hawker Name" />
                  <TableColumn fx:id="hwkCodeColumn" prefWidth="126.0" text="Hawker Code" />
                  <TableColumn fx:id="hwkMobileColumn" prefWidth="125.0" text="Mobile Number" />
                  <TableColumn fx:id="hwkAgencyColumn" prefWidth="125.0" text="Agency Name" />
                  <TableColumn fx:id="hwkFeeColumn" prefWidth="87.0" text="Fee" />
                  <TableColumn fx:id="hwkActiveColumn" prefWidth="67.0" text="Active" />
                  <TableColumn fx:id="hwkTotalDueColumn" prefWidth="67.0" text="Total Due" />
                  <TableColumn fx:id="hwkOldHouseNumColumn" prefWidth="113.0" text="Old House#" />
                  <TableColumn fx:id="hwkNewHouseNumColumn" prefWidth="113.0" text="New House#" />
                  <TableColumn fx:id="hwkAddrLine1Column" prefWidth="192.0" text="Address Line1" />
                  <TableColumn fx:id="hwkAddrLine2Column" prefWidth="192.0" text="Address Line2" />
                  <TableColumn fx:id="hwkLocalityColumn" prefWidth="192.0" text="Locality" />
                  <TableColumn fx:id="hwkCityColumn" prefWidth="192.0" text="City" />
                  <TableColumn fx:id="hwkStateColumn" prefWidth="192.0" text="State" />
               </columns>
            </TableView>
            <HBox>
               <VBox.margin>
                  <Insets bottom="5.0" top="10.0" />
               </VBox.margin>
               <children>
                  <VBox alignment="TOP_CENTER" HBox.hgrow="ALWAYS">
                     <children>
                        <Label text="Hawker Billing Settlement">
                           <font>
                              <Font size="18.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" />
                           </VBox.margin>
                        </Label>
                        <TableView fx:id="hawkerBillingTable">
                           <columns>
                              <TableColumn fx:id="hwkBillStartDateColumn" prefWidth="105.0" text="Start Date" />
                              <TableColumn fx:id="hwkBillEndDateColumn" prefWidth="94.0" sortType="DESCENDING" text="End Date" />
                              <TableColumn fx:id="hwkBillAmountColumn" prefWidth="83.0" text="Amount" />
                              <TableColumn fx:id="hwkBillPaidColumn" prefWidth="83.0" text="Paid" />
                              <TableColumn fx:id="hwkBillDueColumn" prefWidth="83.0" text="Due" />
                           </columns>
                           <VBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </VBox.margin>
                        </TableView>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Label text="Screen Access">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <ButtonBar prefHeight="40.0" prefWidth="200.0">
                          <buttons>
                            <Button fx:id="enableAllButton" mnemonicParsing="false" onAction="#enableAllClicked" text="Enable All" />
                              <Button fx:id="disableAllButton" layoutX="411.0" layoutY="18.0" mnemonicParsing="false" onAction="#disableAllClicked" text="Disable All" />
                          </buttons>
                           <VBox.margin>
                              <Insets right="10.0" />
                           </VBox.margin>
                        </ButtonBar>
                        <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
                           <children>
                              <GridPane alignment="CENTER" VBox.vgrow="ALWAYS">
                                 <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                 </columnConstraints>
                                 <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                 </rowConstraints>
                                 <children>
                                    <CheckBox fx:id="customerCheckBox" mnemonicParsing="false" text="Customers" />
                                    <CheckBox fx:id="lineInfoCheckBox" mnemonicParsing="false" text="Line Information" GridPane.columnIndex="2" />
                                    <CheckBox fx:id="lineDistCheckBox" mnemonicParsing="false" text="Line Distributor" GridPane.rowIndex="1" />
                                    <CheckBox fx:id="pausedCustCheckBox" mnemonicParsing="false" text="Paused Customers" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                    <CheckBox fx:id="prodCatalogCheckBox" mnemonicParsing="false" text="Product Catalog" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                                    <CheckBox fx:id="billingCheckBox" mnemonicParsing="false" text="Billing" GridPane.columnIndex="1" />
                                    <CheckBox fx:id="reportsCheckBox" mnemonicParsing="false" text="Reports" GridPane.rowIndex="2" />
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </GridPane>
                           </children>
                        </VBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
